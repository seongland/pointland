{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/tokyo-potree/:path*",
      "destination": "https://storage.googleapis.com/tokyo-potree/:path*"
    }
  ]
}
