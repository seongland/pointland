{
  "name": "pointland",
  "version": "1.3.1",
  "private": true,
  "description": "Web Metaverse with Point Cloud",
  "author": "seonglae",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "start": "cross-env NODE_ENV=production nuxt",
    "production": "pm2 delete pointland & pm2 start process.json",
    "restart": "pm2 restart pointland",
    "stop": "pm2 delete pointland",
    "build": "cross-env NODE_ENV=production nuxt build",
    "generate": "cross-env NODE_ENV=production nuxt generate",
    "lint": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "format": "prettier --write \"**/*.{ts,json,md,vue}\"  --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "consola": "^2.15.0",
    "core-js": "^3.12.1",
    "layerspace": "^0.2.5",
    "nipplejs": "^0.9.0",
    "nuxt": "^2.15.5",
    "nuxt-vuex-localstorage": "^1.2.6",
    "v-snackbars": "^3.1.0",
    "vue-gtag": "^1.11.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.6",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.11.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^5.2.0",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.7.0",
    "nodemon": "^1.18.9",
    "prettier": "^2.3.0"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": 8080
    }
  },
  "nodemonConfig": {
    "watch": [
      "server/*",
      "./*"
    ],
    "ignore": [
      "build/*"
    ],
    "ext": "ts,json",
    "exec": "nuxt --optimize_for_size"
  }
}
